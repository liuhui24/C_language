#include<stdio.h>
int main(void)
{
    int prefix,idetifier,code,number,digit;
    printf("Enter ISBN: ");
    scanf("%d-%d-%d-%d-%d",&prefix,&idetifier,&code,&number,&digit);
    printf("GS1 prefix: %d\nGroup identifier: %d\nPublisher code: %d\nItem number: %d\nCheck digit: %d",prefix,idetifier,code,number,digit);
    return 0;

}
